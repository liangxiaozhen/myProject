package com.ptpl.model;

import java.math.BigDecimal;

public class PromoTotalRestrict {
	//ID
    private BigDecimal id;
    //限制推广人数方式
    private Short restricttype;
    //推广总人数
    private Long totalnumlimit;
    //最大推广层级
    private Short levellimit;
    //历史最大推广层级
    private Short ismaxlevel;
    public BigDecimal getId() {
        return id;
    }

    public void setId(BigDecimal id) {
        this.id = id;
    }

    public Short getRestricttype() {
        return restricttype;
    }

    public void setRestricttype(Short restricttype) {
        this.restricttype = restricttype;
    }

    public Long getTotalnumlimit() {
        return totalnumlimit;
    }

    public void setTotalnumlimit(Long totalnumlimit) {
        this.totalnumlimit = totalnumlimit;
    }

    public Short getLevellimit() {
        return levellimit;
    }

    public void setLevellimit(Short levellimit) {
        this.levellimit = levellimit;
    }

	public Short getIsmaxlevel()
	{
		return ismaxlevel;
	}

	public void setIsmaxlevel(Short ismaxlevel)
	{
		this.ismaxlevel = ismaxlevel;
	}
}