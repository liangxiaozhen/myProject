package com.ptpl.model;

import java.math.BigDecimal;

//充值定额手续费设置
public class RechargeQuotaFee {
	private BigDecimal id;
	private	BigDecimal	rrid; //充值费率表ID
	private Double	minmoney;//分段最低金额
	private Double	maxmoney;//分段最高金额
	private Double	quotafee;//定额手续费
	public BigDecimal getId() {
		return id;
	}
	public void setId(BigDecimal id) {
		this.id = id;
	}
	public BigDecimal getRrid() {
		return rrid;
	}
	public void setRrid(BigDecimal rrid) {
		this.rrid = rrid;
	}
	public Double getMinmoney() {
		return minmoney;
	}
	public void setMinmoney(Double minmoney) {
		this.minmoney = minmoney;
	}
	public Double getMaxmoney() {
		return maxmoney;
	}
	public void setMaxmoney(Double maxmoney) {
		this.maxmoney = maxmoney;
	}
	public Double getQuotafee() {
		return quotafee;
	}
	public void setQuotafee(Double quotafee) {
		this.quotafee = quotafee;
	}
	@Override
	public String toString() {
		return "RechargeQuotaFee [id=" + id + ", rrid=" + rrid + ", minmoney=" + minmoney + ", maxmoney=" + maxmoney
				+ ", quotafee=" + quotafee + "]";
	}
	
	
}
