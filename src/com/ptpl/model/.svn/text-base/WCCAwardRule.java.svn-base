public class WCCAwardRule {
	/*ID（主键）*/
    private BigDecimal id;
    /*Number(19)注册活动ID*/
    private BigDecimal actid;
    /*Varchar2(200)会员等级(100000000000000000000000000000)*/
    private String ugrade;
    /*Number(5)注册后时间限制*/
    private Integer finishtime;
    /*Number(19)奖品的ID*/
    private BigDecimal awardid;
    /*Varchar2(200)奖品的名称*/
    private String awardname;
    /*Number(5)奖品份数*/
    private Integer awardcopies;
    /*Number(2)奖励方式（定额，投资金额的百分比）*/
    private Short awardtype;
    /*Float奖励定额*/
    private Double quota;
    /*Float奖励百分比数*/
    private Double awardratio;
    /*Float奖励最低值*/
    private Double awardmin;
    /*Float奖励最高值*/
    private Double awardmax;
    /*Number(2)发放方式（系统，人工）*/
    private Short distributetype;
    /*Varchar(50)制表人*/
    private String addman;
    /*Date制表时间*/
    private Date addtime;
    /*Varchar(200)备注*/
    private String remark;
    /*处理前台传递过来的会员等级*/
    private String[] ugradesx;
    /*处理前台传递过来的排除名单*/
    private String[] removenamenorules;

    public BigDecimal getId() {
        return id;
    }

    public void setId(BigDecimal id) {
        this.id = id;
    }

    public BigDecimal getActid() {
        return actid;
    }

    public void setActid(BigDecimal actid) {
        this.actid = actid;
    }

    public String getUgrade() {
        return ugrade;
    }

    public void setUgrade(String ugrade) {
        this.ugrade = ugrade == null ? null : ugrade.trim();
    }

    public Integer getFinishtime() {
        return finishtime;
    }

    public void setFinishtime(Integer finishtime) {
        this.finishtime = finishtime;
    }

    public BigDecimal getAwardid() {
        return awardid;
    }

    public void setAwardid(BigDecimal awardid) {
        this.awardid = awardid;
    }

    public String getAwardname() {
        return awardname;
    }

    public void setAwardname(String awardname) {
        this.awardname = awardname == null ? null : awardname.trim();
    }

    public Integer getAwardcopies() {
        return awardcopies;
    }

    public void setAwardcopies(Integer awardcopies) {
        this.awardcopies = awardcopies;
    }

    public Short getAwardtype() {
        return awardtype;
    }

    public void setAwardtype(Short awardtype) {
        this.awardtype = awardtype;
    }

    public Double getQuota() {
        return quota;
    }

    public void setQuota(Double quota) {
        this.quota = quota;
    }

    public Object getAwardratio() {
        return awardratio;
    }

    public void setAwardratio(Double awardratio) {
        this.awardratio = awardratio;
    }

    public Double getAwardmin() {
        return awardmin;
    }

    public void setAwardmin(Double awardmin) {
        this.awardmin = awardmin;
    }

    public Object getAwardmax() {
        return awardmax;
    }

    public void setAwardmax(Double awardmax) {
        this.awardmax = awardmax;
    }

    public Short getDistributetype() {
        return distributetype;
    }

    public void setDistributetype(Short distributetype) {
        this.distributetype = distributetype;
    }

    public String getAddman() {
        return addman;
    }

    public void setAddman(String addman) {
        this.addman = addman == null ? null : addman.trim();
    }

    public Date getAddtime() {
        return addtime;
    }

    public void setAddtime(Date addtime) {
        this.addtime = addtime;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark == null ? null : remark.trim();
    }

	public String[] getUgradesx() {
		return ugradesx;
	}

	public void setUgradesx(String[] ugradesx) {
		this.ugradesx = ugradesx;
	}

	public String[] getRemovenamenorules() {
		return removenamenorules;
	}

	public void setRemovenamenorules(String[] removenamenorules) {
		this.removenamenorules = removenamenorules;
	}
    
    
    
}