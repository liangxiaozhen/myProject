package com.ptpl.model;

import com.alibaba.fastjson.annotation.JSONField;

/**余额查询返回实体类*/
public class QueryBalanceBg {
	private String  CmdId;
	private String	RespCode;
	private String	RespDesc;
	private String	MerCustId;
	private String	UsrCustId;
	/**可用余额*/
	private String	AvlBal;
	/**账户余额*/
	private String	AcctBal;
	/**冻结余额*/
	private String	FrzBal;
	private String PlainStr;
	private String	ChkValue;
	public String getCmdId() {
		return CmdId;
	}
	@JSONField(name="CmdId")
	public void setCmdId(String cmdId) {
		CmdId = cmdId;
	}
	public String getRespCode() {
		return RespCode;
	}
	@JSONField(name="RespCode")
	public void setRespCode(String respCode) {
		RespCode = respCode;
	}
	public String getRespDesc() {
		return RespDesc;
	}
	@JSONField(name="RespDesc")
	public void setRespDesc(String respDesc) {
		RespDesc = respDesc;
	}
	public String getMerCustId() {
		return MerCustId;
	}
	@JSONField(name="MerCustId")
	public void setMerCustId(String merCustId) {
		MerCustId = merCustId;
	}
	public String getUsrCustId() {
		return UsrCustId;
	}
	@JSONField(name="UsrCustId")
	public void setUsrCustId(String usrCustId) {
		UsrCustId = usrCustId;
	}
	public String getAvlBal() {
		return AvlBal;
	}
	@JSONField(name="AvlBal")
	public void setAvlBal(String avlBal) {
		AvlBal = avlBal;
	}
	public String getAcctBal() {
		return AcctBal;
	}
	@JSONField(name="AcctBal")
	public void setAcctBal(String acctBal) {
		AcctBal = acctBal;
	}
	public String getFrzBal() {
		return FrzBal;
	}
	@JSONField(name="FrzBal")
	public void setFrzBal(String frzBal) {
		FrzBal = frzBal;
	}
	public String getChkValue() {
		return ChkValue;
	}
	@JSONField(name="ChkValue")
	public void setChkValue(String chkValue) {
		ChkValue = chkValue;
	}
	public String getPlainStr() {
		return PlainStr;
	}
	@JSONField(name="PlainStr")
	public void setPlainStr(String plainStr) {
		PlainStr = plainStr;
	}
	@Override
	public String toString() {
		return "QueryBalanceBg [CmdId=" + CmdId + ", RespCode=" + RespCode + ", RespDesc=" + RespDesc + ", MerCustId="
