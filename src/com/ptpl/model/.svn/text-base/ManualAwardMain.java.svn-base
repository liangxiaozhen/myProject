package com.ptpl.model;

import java.math.BigDecimal;
import java.util.Date;

public class ManualAwardMain {
	
	private BigDecimal id;//id
	
	private String activityNo;//活动编号
	 
	private String activityName;//活动名称
	 
	private Short actMType;//活动生成方式  1.手动 2.模板
	 
	private Date activityTime;//活动执行时间（精确到秒）
	
	private String activityTimeStr;//活动执行时间字符串（用在前台显示）
	
	private Short isManual;//是否容许手动执行   1.容许  2.不容许
	 
	private Short isDealMain;//活动状态   0.待完善  1.待执行  2.执行中（弃用）  3.已完成   4.已停用   5.已作废   6.已过期
	 
	private Date addTimeMain;//活动设置时间
	
	private String addTimeMainStr;//活动设置时间字符串（用在前台显示）
	 
	private String addManMain;//活动设置人
	 
	private Short isTempletMain;//是否为模板（1是，2否）
	
	private Short subActivityNum;//子活动数
	
	private Short executeStatus;//执行状态   1.手工执行   2.系统执行
	
	private String remark;//活动备注
	
	private Short isAudit;//获奖名单   是否需要审核   1.是   2.否

	public BigDecimal getId() {
		return id;
	}

	public void setId(BigDecimal id) {
		this.id = id;
	}

	public String getActivityNo() {
		return activityNo;
	}

	public void setActivityNo(String activityNo) {
		this.activityNo = activityNo;
	}

	public String getActivityName() {
		return activityName;
	}

	public void setActivityName(String activityName) {
		this.activityName = activityName;
	}

	public Short getActMType() {
		return actMType;
	}

	public void setActMType(Short actMType) {
		this.actMType = actMType;
	}

	public Date getActivityTime() {
		return activityTime;
	}

	public void setActivityTime(Date activityTime) {
		this.activityTime = activityTime;
	}

	public String getActivityTimeStr() {
		return activityTimeStr;
	}

	public void setActivityTimeStr(String activityTimeStr) {
		this.activityTimeStr = activityTimeStr;
	}

	public Short getIsManual() {
		return isManual;
	}

	public void setIsManual(Short isManual) {
		this.isManual = isManual;
	}

	public Short getIsDealMain() {
		return isDealMain;
	}

	public void setIsDealMain(Short isDealMain) {
		this.isDealMain = isDealMain;
	}

	public Date getAddTimeMain() {
		return addTimeMain;
	}

	public void setAddTimeMain(Date addTimeMain) {
		this.addTimeMain = addTimeMain;
	}

	public String getAddTimeMainStr() {
		return addTimeMainStr;
	}

	public void setAddTimeMainStr(String addTimeMainStr) {
		this.addTimeMainStr = addTimeMainStr;
	}

	public String getAddManMain() {
		return addManMain;
	}

	public void setAddManMain(String addManMain) {
		this.addManMain = addManMain;
	}

	public Short getIsTempletMain() {
		return isTempletMain;
	}

	public void setIsTempletMain(Short isTempletMain) {
		this.isTempletMain = isTempletMain;
	}

	public Short getSubActivityNum() {
		return subActivityNum;
	}

	public void setSubActivityNum(Short subActivityNum) {
		this.subActivityNum = subActivityNum;
	}

	public Short getExecuteStatus() {
		return executeStatus;
	}

	public void setExecuteStatus(Short executeStatus) {
		this.executeStatus = executeStatus;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public Short getIsAudit() {
		return isAudit;
	}

	public void setIsAudit(Short isAudit) {
		this.isAudit = isAudit;
	}

	@Override
	public String toString() {
		return "ManualAwardMain [id=" + id + ", activityNo=" + activityNo + ", activityName=" + activityName
				+ ", actMType=" + actMType + ", activityTime=" + activityTime + ", activityTimeStr=" + activityTimeStr
				+ ", isManual=" + isManual + ", isDealMain=" + isDealMain + ", addTimeMain=" + addTimeMain
				+ ", addTimeMainStr=" + addTimeMainStr + ", addManMain=" + addManMain + ", isTempletMain="
				+ isTempletMain + ", subActivityNum=" + subActivityNum + ", executeStatus=" + executeStatus
				+ ", remark=" + remark + ", isAudit=" + isAudit + "]";
	}

}
