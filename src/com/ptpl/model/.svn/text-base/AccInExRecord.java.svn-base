package com.ptpl.model;

import java.math.BigDecimal;
import java.util.Date;

public class AccInExRecord {
	//ID
    private BigDecimal id;
    //用户ID
    private BigDecimal baseid;
    //平台账户金额，平台产生费用，进账为+ 出账为-
    private Double amount;
    //平台进账
    private Double pinamount;
    //平台出账
    private Double poutamount;
    //业务类型  1 充值，2 充值手续费，3 提现，4 提现手续费,5 投标，6 还款,7 会员升级费，8 提现服务费，
    private Short type;
    //用户可用余额
    private Double balance;
    //收入
    private Double inamount;
    //支出
    private Double outamount;
    //发生时间
    private Date recordtime;
    //说明
    private String description;
    //备注
    private String remark;
    //用户基本信息
    private UserBaseAccountInfo ubai;
    //用户冻结余额
    private Double freebalance;
    //用户总余额
    private Double totalbalance;
    //页面查询开始时间
    private Date btime;
    //页面查询结束时间
    private Date etime;
    //状态（摘要） 0冻结  1成功 2失败
    private Short status;
    //流水号
    private String aieorderno; 
    /** 平台账户 */
    private String paccount;
    /*业务流水号*/
    private String borderno;
    /*符号*/
    private String sign;
    /*用户托管信息对象*/
    private UserFsAccountInfo ufai;
    public BigDecimal getId() {
        return id;
    }

    public void setId(BigDecimal id) {
        this.id = id;
    }

    public BigDecimal getBaseid() {
        return baseid;
    }

    public void setBaseid(BigDecimal baseid) {
        this.baseid = baseid;
    }

    public Double getAmount() {
        return amount;
    }

    public void setAmount(Double amount) {
        this.amount = amount;
    }

    public Short getType() {
        return type;
    }

    public void setType(Short type) {
        this.type = type;
    }

    public Double getBalance() {
        return balance;
    }

    public void setBalance(Double balance) {
        this.balance = balance;
    }

    public Date getRecordtime() {
        return recordtime;
    }

    public void setRecordtime(Date recordtime) {
        this.recordtime = recordtime;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark == null ? null : remark.trim();
    }

	public Double getInamount() {
		return inamount;
	}

	public void setInamount(Double inamount) {
		this.inamount = inamount;
	}

	public Double getOutamount() {
		return outamount;
	}

	public void setOutamount(Double outamount) {
		this.outamount = outamount;
	}
	
	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public UserBaseAccountInfo getUbai() {
		return ubai;
	}

	public void setUbai(UserBaseAccountInfo ubai) {
		this.ubai = ubai;
	}

	public Date getBtime()
	{
		return btime;
	}

	public void setBtime(Date btime)
	{
		this.btime = btime;
	}

	public Date getEtime()
	{
		return etime;
	}

	public void setEtime(Date etime)
	{
		this.etime = etime;
	}

	public Double getFreebalance()
	{
		return freebalance;
	}

	public void setFreebalance(Double freebalance)
	{
		this.freebalance = freebalance;
	}

	public Double getTotalbalance()
	{
		return totalbalance;
	}

	public void setTotalbalance(Double totalbalance)
	{
		this.totalbalance = totalbalance;
	}

	public Double getPinamount() {
		return pinamount;
	}

	public void setPinamount(Double pinamount) {
		this.pinamount = pinamount;
	}

	public Double getPoutamount() {
		return poutamount;
	}

	public void setPoutamount(Double poutamount) {
		this.poutamount = poutamount;
	}

	public Short getStatus()
	{
		return status;
	}

	public void setStatus(Short status)
	{
		this.status = status;
	}

	public String getAieorderno()
	{
		return aieorderno;
	}

	public void setAieorderno(String aieorderno)
	{
		this.aieorderno = aieorderno;
	}

	public String getPaccount() {
		return paccount;
	}

	public void setPaccount(String paccount) {
		this.paccount = paccount;
	}

	public String getBorderno() {
		return borderno;
	}

	public void setBorderno(String borderno) {
		this.borderno = borderno;
	}

	public String getSign() {
		return sign;
	}

	public void setSign(String sign) {
		this.sign = sign;
	}

	public UserFsAccountInfo getUfai() {
		return ufai;
	}

	public void setUfai(UserFsAccountInfo ufai) {
		this.ufai = ufai;
	}
	
	
}