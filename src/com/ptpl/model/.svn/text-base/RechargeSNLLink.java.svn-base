package com.ptpl.model;

import java.math.BigDecimal;

//充值排除名单关联表
public class RechargeSNLLink {
	private BigDecimal id;//	ID(主键）
	private BigDecimal	rrid;//充值费率表ID
	private BigDecimal	snlid;//定向名单表id
	private String code;
	private String codename;
	public BigDecimal getId() {
		return id;
	}
	public void setId(BigDecimal id) {
		this.id = id;
	}
	public BigDecimal getRrid() {
		return rrid;
	}
	public void setRrid(BigDecimal rrid) {
		this.rrid = rrid;
	}
	public BigDecimal getSnlid() {
		return snlid;
	}
	public void setSnlid(BigDecimal snlid) {
		this.snlid = snlid;
	}
	
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	
	public String getCodename() {
		return codename;
	}
	public void setCodename(String codename) {
		this.codename = codename;
	}
	@Override
	public String toString() {
		return "RechargeSNLLink [id=" + id + ", rrid=" + rrid + ", snlid=" + snlid + ", code=" + code + ", codename="
				+ codename + "]";
	}
	
}
