package com.ptpl.constant;

import java.util.HashMap;
import java.util.Map;

import com.alibaba.fastjson.JSON;

public class ActivityAwardList_Constant {
	// 奖品状态（发放，待发，取消）
	public static final Short STATUS_FANFANG = 1;
	public static final Short STATUS_DAIFANG = 2;
	public static final Short STATUS_QUXIAO = 3;

	// 定义一个map用于存放奖品状态
	public static Map<Short, String> status_map;

	static {
		status_map = new HashMap<Short, String>();
		status_map.put(STATUS_FANFANG, "待发");
		status_map.put(STATUS_DAIFANG, "发放");
		status_map.put(STATUS_QUXIAO, "取消");
	}

}
