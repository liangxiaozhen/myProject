<style type="text/css">
    #bannerWrap ul.buttonDot{margin-left: -75px;}
    /**轮播图 s**/
    .niwdoawi_banner{height:350px; min-width:1180px; position:relative; overflow:hidden;}
    .niwdoawi_banner .img{position:relative; z-index:0;}
    .niwdoawi_banner .page{position:absolute; bottom:15px;left:0;text-align: center; z-index:99;width: 100%;}
    .niwdoawi_banner .page span{cursor:pointer; display:inline-block; width:15px; height:15px;  margin-right:15px;background-color:#DFDFDF;font-size:0;overflow: hidden;border-radius: 15px;}
    .niwdoawi_banner .page span.current{background-color:#F60}
    .niwdoawi_banner .prev{position:absolute; top:0; height:350px; z-index:101; left:0; width:50px; background:url(${pageContext.request.contextPath}/resources/resource/Images/arrow-left-light.png) 23px 150px no-repeat; cursor:pointer; opacity:.3;filter:alpha(opacity=30);-moz-opacity:0.3;-khtml-opacity: 0.3; transition:all .5s}
    .niwdoawi_banner .next{position:absolute; top:0; height:350px; z-index:101; right:0; width:50px; background:url(${pageContext.request.contextPath}/resources/resource/Images/arrow-right-light.png) 2px 150px no-repeat; cursor:pointer; opacity:.3;filter:alpha(opacity=30);-moz-opacity:0.3;-khtml-opacity: 0.3;transition:all .5s}
    .niwdoawi_banner .prev:hover,.niwdoawi_banner .next:hover{opacity:.8;filter:alpha(opacity=80);-moz-opacity:0.8;-khtml-opacity: 0.8;}
    .niwdoawi_banner .prev:hover{ background-position:8px 150px;}
    .niwdoawi_banner .next:hover{ background-position:12px 150px;}
    .niwdoawi_banner ul li{ width:100%; height:300px;text-align:center;}
    .niwdoawi_banner ul li a{ width:100%; height:100%; display: block}
    /**轮播图 e**/
    #loanMain .explainPro .leftTitle {float: left;}
    .banner1{background: url(${pageContext.request.contextPath}/resources/resource/Images/1.png) no-repeat center;display: none}
    .banner2{background: url(${pageContext.request.contextPath}/resources/resource/Images/2.png) no-repeat center;display: none}
