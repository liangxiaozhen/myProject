<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@include file="/WEB-INF/jsp/common/taglib.jsp"%>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>用户还款 参数设置 测试页面</title>
<%@include file="/WEB-INF/jsp/common/public.jsp"%>
</head>
<body>
	<div id="box">
		<form id="Repayment">
			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">标的ID
						ProId:&nbsp;&nbsp;&nbsp;<span class="red">*</span>
					</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="ProId"
							value="gjbh201609051627" />
					</div>
				</div>
			</div>

			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">出账客户号
						(借款人)OutCustId:&nbsp;&nbsp;&nbsp;<span class="red">*</span>
					</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="OutCustId"
							value="6000060005290553" />
					</div>
				</div>
			</div>
			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">订单号(投标订单号)
						SubOrdId:&nbsp;&nbsp;&nbsp;<span class="red">*</span>
					</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="SubOrdId"
							value="20160924165458939243" />
					</div>
				</div>
			</div>
			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">订单日期
						(投标订单日期)SubOrdDate：&nbsp;&nbsp;&nbsp;<span class="red">*</span>
					</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="SubOrdDate"
							value="20160924" />
					</div>
				</div>
			</div>

			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">还款本金
						(格式1.00)PrincipalAmt:&nbsp;&nbsp;&nbsp;<span class="red">*</span>
					</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="PrincipalAmt"
							value="10.00" />
					</div>
				</div>
			</div>
			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">还款利息
						(格式1.00)InterestAmt:&nbsp;&nbsp;&nbsp;<span class="red">*</span>
					</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="InterestAmt"
							value="0.00" />
					</div>
				</div>
			</div>

			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">入账客户号(投资人)
						InCustId:&nbsp;&nbsp;&nbsp;<span class="red">*</span>
					</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="InCustId"
							value="6000060004190334" />
					</div>
				</div>
			</div>

			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">垫资/代偿对象
						(如果垫资 填原借款人汇付客户号)DzObject:</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="DzObject" value="" />
					</div>
				</div>
			</div>
			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">扣款手续费
						Fee:&nbsp;&nbsp;&nbsp;<span class="red">*</span>
					</label>
					<div class="col-sm-2">
						<input type="text" class="form-control" name="Fee" value="1.00" />
					</div>
					<span class="red">如果扣款手续费Fee不等于 != 0.00 以下选项必填(注意格式)</span>
				</div>
			</div>
			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">分账商户号
						DivCustId:</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="DivCustId"
							value="6000060004166478" />
					</div>
				</div>
			</div>
			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">分账账户号
						DivAcctId:</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="DivAcctId"
							value="SDT000001" />
					</div>
				</div>
			</div>
			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">分账金额
						DivAmt:</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="DivAmt" value="1.00" />
					</div>
				</div>
			</div>
			<div class="row" style="margin-top: 10px;">
				<div class="col-md-8">
					<label for="exampleInputName2" class="col-sm-2 text-right">手续费收取对象标志
						I/O(I--向入款客户号 InCustId(投资人) 收取O--向出款客户号 OutCustId(还款人) 收取):</label>
					<div class="col-sm-6">
						<input type="text" class="form-control" name="FeeObjFlag"
							value="O" />
					</div>
				</div>
			</div>

			<button class="btn col-md-offset-2 " id="submit" type="button"
				onclick="gjksubmit(this)">&nbsp;&nbsp;&nbsp;提交&nbsp;&nbsp;&nbsp;</button>
			<div class="row" style="margin-top: 130px;">
				<div class="col-md-8">
					<div class="col-sm-6"></div>
				</div>
			</div>
		</form>
	</div>
	<script type="text/javascript">
 	   function gjksubmit(){
 		   var params = $("#Repayment").serialize();
 		  $.tzAjax.request({
 			  model:"huifu/repayMent",
 			  method:"test2.action",
 			  callback:function(data){
  				  $("#box").html(data);
 			  }
  		  },params);
 	   }
 </script>
</body>
</html>