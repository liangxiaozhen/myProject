body{background:yellow url(../Images/bodybg.png) left top repeat;}
.clearfix{display:block;zoom:1}
.bannerCon{background: url(../images/bannerbg.png) top center no-repeat; margin-bottom: 30px;}
.stepCon{width: 100%; text-align: center; background: rgba(212,0,9,0.9); padding: 150px 0 35px 0;}
.stepCon h4{color: #ffbe0d; font-size: 20px; margin-bottom: 10px; padding-top: 10px;}
.stepCon ul{width:680px; margin: 0 auto;}
.stepCon li{width: 325px; float: left; color: #fff;display: block; text-align: center;}
.stepCon li a{transition: all 1.3s; position: relative; width: 170px; color: #fff;display:inline-block;}
.stepCon li span{display: inline-block; width:142px; height: 142px; background: url(../images/stepimg.png) top center no-repeat;}
.stepCon li span i{display: block; width:65px; height: 65px; background: url(../images/steppic.png) left top no-repeat; margin: 0 auto; margin-top: 40px;}
.stepCon li span i.weixin{background-position: -37px -32px;}
.stepCon li span i.weblink{background-position: -188px -32px;}
.stepCon li span i.webtel{background-position: -339px -32px;}
.stepCon li span i.webqq{background-position: -491px -32px;}
