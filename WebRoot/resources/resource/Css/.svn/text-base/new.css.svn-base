/* 侧边导航  s */
.side_nav{position: relative;width: 100%;}
.side_nav .nav_name{width: 100%; position: relative; z-index: 10;}
.side_nav .nav_name a{display: block; text-align: center; position: relative; z-index: 10;}
.side_nav .nav_name dt{width: 100%;}
.side_nav .nav_name dt a{ background-color: #fafafa; color: #999; border-top: solid 1px #fafafa; border-bottom: solid 1px #fafafa;padding-left:0px; position: absolute; left: 0; top: 0; padding-right: 10px; font-size: 16px; font-weight: bold; text-decoration: none; position: relative; z-index: 5; transition: padding-left 0.5s;}
.side_nav .nav_name dt i{position: absolute; right: -2px; top: 0; display:none; background: #fff; height: 44px; width: 4px; z-index: 2;}
.side_nav .nav_name dt a i{display:block; position: absolute; background-color: #f00; height: 44px; width: 4px; right: -2px; top:0;}
.side_nav .nav_name dt a:hover{background-color: #f5f5f5;border-top: solid 1px #e6e6e6; border-bottom: solid 1px #e6e6e6;padding-left: 20px;}
.side_nav .navcurron dt a,.side_nav .navcurron dt a:hover{background-color: #fff;  padding-left: 20px;border-top: solid 1px #e6e6e6; border-bottom: solid 1px #e6e6e6; color:#333;}

.side_nav .nav_name dd{position: absolute; height: auto; width:130px; background-color: #fff;right: -132px; top:0; display:none; z-index: 12; padding: 15px 0;box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.15);}
.side_nav .nav_name dd a{padding: 5px 0; color:#999;}
.side_nav .nav_name:hover dd{display: block;}
.side_nav .nav_name dd i{width: 4px; height: 44px; background-color: #fff; display: block; position: absolute; left: -4px; top: 0;}
.side_nav .nav_name dd a:hover{color:#548de7;}
.side_nav .navcurron dt i{display: inline-block;}
