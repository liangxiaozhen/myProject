  .version-switch-pop-wrapper {
        position: fixed;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        z-index: 999;
    }
    .version-switch-pop-wrapper .mask {
        position: absolute;
        left: 0;
        top: 0;
        z-index: 50;
        width: 100%;
        height: 100%;
        background-color: #000a1a;
        opacity: 0.5;
        filter: alpha(opacity=50);
    }
    .version-switch-pop-wrapper .icon-close {
        display: inline-block;
        float: right;
        width: 14px;
        height: 14px;
        margin-top: 12px;
        cursor: pointer;
        background: url(Images/icon_switch_close.png) no-repeat;
        transition: all .5s ease;
    }
    .version-switch-pop-wrapper .icon-close:hover {
        transform: rotate(180deg);
    }
    .version-switch-pop-wrapper .version-switch-pop {
        position: absolute;
        left: 50%;
        top: 50%;
        z-index: 100;
        width: 500px;
        margin-left: -250px;
        margin-top: -177px;
        background-color: #fff;
        border-radius: 2px;
    }
    .version-switch-pop-wrapper .version-switch-pop .pop-header {
        height: 40px;
        line-height: 40px;
        background-color: #1b2b46;
        padding: 0 15px;
        font-size: 16px;
        color: #fff;
    }
    .version-switch-pop-wrapper .version-switch-pop .pop-body {
        height: 315px;
        text-align: center;
        padding-top: 54px;

    }
    .version-switch-pop-wrapper .pop-body img {
        width: 252px;
        height: 60px;
    }
    .version-switch-pop-wrapper .pop-body .pop-text {
        margin-top: 28px;
        line-height: 30px;
        font-size: 14px;
        color: #666;
    }
    .version-switch-pop-wrapper .pop-body .pop-btns {
        margin-top: 32px;
        text-align: center;

    }
    .version-switch-pop-wrapper .pop-btns a {
        display: inline-block;
        width: 120px;
        height: 40px;
        line-height: 40px;
        border: 1px solid #e0e4ed;
        border-radius: 2px;
        font-size: 16px;
        color: #1b2b46;
    }
    .version-switch-pop-wrapper .pop-btns a:hover {
        border-color: #cdd2dc;
    }
    .version-switch-pop-wrapper .pop-btns a.default {
        border: none;
        background-color: #e55;
        color: #fff;
    }
    .version-switch-pop-wrapper .pop-btns a.default:hover {
        background-color: #e44;
    }
    .version-switch-pop-wrapper .pop-btns a.left {
        margin-right: 20px;
    }
